!!!
%html
  %head
    %title GodockRails4Angularjs
    = stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags
  %body
    #wrap{"ng-app" => "godock"}
      .navbar.navbar-default.navbar-fixed-top.bs-docs-nav{:role => "navigation"}
        .container
          .navbar-header
            %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", :type => "button"}
              %span.sr-only Toggle navigation
              %span.icon-bar
              %span.icon-bar
              %span.icon-bar
            %a.navbar-brand{:href => "#"} GoDock
          %div{"search-results" => ""}
      #content.bs-header(ng-controller="SearchCntl")
        .search-results-list
          .search-group.col-md-4{"ng-repeat" => "func in searchResult.Function"}
            .func {{func.name}}
          .search-group.col-md-4{"ng-repeat" => "func in searchResult.TypeClass"}
            .tc {{func.name}}
          .search-group.col-md-4{"ng-repeat" => "func in searchResult.Namespace"}
            .ns {{func.name}}
      .container
        %ul.nav.nav-tabs
          %li.active
            %a{:href => "#"} gopkg
          %li
            %a{:href => "#"} martini
          %li
            %a{:href => "#"} mgo

      .container
        = yield
      #footer
        .container
          %p.text-muted &copy;Copyright 2013 Isaiah P.
